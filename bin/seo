#!/usr/bin/env node
'use strict'

var Service = require('../')

var service = new Service({
  url: process.argv[2],
  strategy: process.argv[3]
})
service.on('sitemap', (loc) => {
  console.log(`produced sitemap (${loc})`)
})
service.on('jsonlds', (dir) => {
  console.log(`produced jsonld files at ${dir}`)
})
service.on('all', () => {
  console.log('produced all assets')
})
service.start()
